{
  "name": "@tech-consortium/ngx-panzoom",
  "version": "17.0.0",
  "author": "Tim Underhay",
  "license": "MIT",
  "homepage": "https://github.com/tech-consortium/ngx-panzoom",
  "repository": {
    "type": "git",
    "url": "https://github.com/tech-consortium/ngx-panzoom.git"
  },
  "engines": {
    "node": "^20.10.0 || ^21.1.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --configuration production",
    "lint": "ng lint",
    "lintfix": "ng lint --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "e2e": "ng e2e",
    "buildpack": "rm -rf dist/ && ng build --configuration production && cd dist/ngx-panzoom && npm pack .",
    "buildpublish": "rm -rf dist/ && ng build --configuration production && cd dist/ngx-panzoom && npm publish .",
    "builddemotest": "VER=`grep version projects/ngx-panzoom/package.json | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'` && npm run buildpack && cp dist/ngx-panzoom/ngx-panzoom-*.tgz ~/src/ngx-panzoom-demo && cd ~/src/ngx-panzoom-demo && pnpm add ./ngx-panzoom-${VER}.tgz"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "~17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "~17.0.5",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "ng-packagr": "^17.0.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.5",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "~17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/language-service": "^17.0.5",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "ts-node": "~10.9.1",
    "typescript": "~5.2.2"
  },
  "lint-staged": {
    "*.{ts,html,css,json,md}": "prettier --write"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}